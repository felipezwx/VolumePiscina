programa {
  funcao inicio() {

    real c, l, p, v

    escreva("Insira o comprimento da piscina : ")
    leia(c)
    limpa()

    escreva("Insira a largura da piscina : ")
    leia(l)
    limpa()
  

    escreva("Insira a profundidade da piscina : ")
    leia(p)
    limpa()
   
    v = ( c * l * p )

    escreva("O volume da piscina e de : ", v,"m³")

    
  }
}
